<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.personal.quantization.center.mapper.CenterTestMapper">

  <resultMap id="centerTestMap" type="com.personal.quantization.model.CenterTest">
    <id column="center_id" property="centerId" jdbcType="VARCHAR"/>
    <result column="center_code" property="centerCode" jdbcType="VARCHAR"/>
    <result column="center_name" property="centerName" jdbcType="VARCHAR"/>
  </resultMap>
  
  <insert id="saveCenterTest" parameterType="com.personal.quantization.model.CenterTest">
  	insert into center_test(center_code, center_name)
  	values (#{centerCode}, #{centerName})
  </insert>
  
  <select id="selectCenterTests" resultMap="centerTestMap">
  	select center_id, center_code, center_name from center_test
  </select>
  
</mapper>